{
  "name": "ur-random",
  "version": "0.0.4",
  "description": "a pseudo random number generator that spawns other pseudo random number generators",
  "main": "dist/index.js",
  "repository": "github.com/chriscauley/ur-random/",
  "homepage": "https://chriscauley.github.io/ur-random/",
  "scripts": {
    "build": "rm -rf docs dist && yarn build-dist && yarn build-docs",
    "build-dist": "parcel build index.js",
    "serve-docs": "parcel demo/index.html -p 8111 -d docs --public-url /ur-random/",
    "build-docs": "parcel build demo/index.html --no-minify -d docs --public-url /ur-random/",
    "lint": "eslint index.js"
  },
  "author": "chriscauley",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-riot": "^0.1.8",
    "parcel": "^1.10.3",
    "parcel-plugin-riot": "https://github.com/chriscauley/parcel-plugin-riot.git",
    "prettier": "^1.18.2"
  }
}
